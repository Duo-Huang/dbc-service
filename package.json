{
    "name": "dbc",
    "version": "0.0.1",
    "description": "",
    "author": "glduo666@163.com",
    "private": true,
    "license": "UNLICENSED",
    "packageManager": "pnpm@10.18.2",
    "engines": {
        "node": ">=20.0.0",
        "pnpm": ">=10.0.0",
        "npm": "please-use-pnpm",
        "yarn": "please-use-pnpm"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "concurrently \"pnpm:build:console\" \"pnpm:build:miniprogram\"",
        "build:console": "cross-env NODE_ENV=production nest build console --builder webpack",
        "build:miniprogram": "cross-env NODE_ENV=production nest build miniprogram --builder webpack",
        "format": "prettier --write \"**/*.{ts,js,mjs,json,yaml,yml,md}\"",
        "start:console": "cross-env NODE_ENV=development nest start console",
        "start:dev:console": "cross-env NODE_ENV=development nest start console --watch",
        "start:debug:console": "cross-env NODE_ENV=development nest start console --debug --watch",
        "start:prod:console": "cross-env NODE_ENV=production node dist/apps/console/main",
        "start:miniprogram": "cross-env NODE_ENV=development nest start miniprogram",
        "start:dev:miniprogram": "cross-env NODE_ENV=development nest start miniprogram --watch",
        "start:debug:miniprogram": "cross-env NODE_ENV=development nest start miniprogram --debug --watch",
        "start:prod:miniprogram": "cross-env NODE_ENV=production node dist/apps/miniprogram/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./jest-e2e.config.ts --verbose",
        "test:e2e:console": "jest --config ./jest-e2e.config.ts apps/console/test --verbose",
        "test:e2e:miniprogram": "jest --config ./jest-e2e.config.ts apps/miniprogram/test --verbose",
        "test:e2e:watch": "jest --config ./jest-e2e.config.ts --watch",
        "test:e2e:cov": "jest --config ./jest-e2e.config.ts --coverage --verbose",
        "test:all": "pnpm test && pnpm test:e2e",
        "migration": "node database/scripts/migration-helper.js",
        "clean": "rimraf dist coverage",
        "prepare": "node -e \"process.env.CI !== 'true' && require('child_process').execSync('husky', {stdio: 'inherit'})\""
    },
    "lint-staged": {
        "**/*.ts": [
            "prettier --write",
            "eslint --fix"
        ]
    },
    "dependencies": {
        "@nestjs/common": "^11.1.6",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.1.6",
        "@nestjs/platform-fastify": "^11.1.6",
        "@nestjs/typeorm": "^11.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.2",
        "config": "^4.1.1",
        "dayjs": "^1.11.18",
        "fastify": "^5.6.1",
        "nestjs-pino": "^4.4.1",
        "pg": "^8.16.3",
        "pino": "^9.13.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "typeorm": "^0.3.27"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.37.0",
        "@nestjs/cli": "^11.0.10",
        "@nestjs/schematics": "^11.0.9",
        "@nestjs/testing": "^11.1.6",
        "@types/config": "^3.3.5",
        "@types/jest": "^30.0.0",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^24.9.2",
        "@types/pg": "^8.15.5",
        "@types/source-map-support": "^0.5.10",
        "@types/supertest": "^6.0.3",
        "concurrently": "^9.2.1",
        "copy-webpack-plugin": "^13.0.1",
        "cross-env": "^10.1.0",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "globals": "^16.4.0",
        "husky": "^9.1.7",
        "jest": "^30.2.0",
        "lint-staged": "^16.2.4",
        "pino-pretty": "^13.1.2",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "source-map-support": "^0.5.21",
        "supertest": "^7.1.4",
        "terser-webpack-plugin": "^5.3.14",
        "ts-jest": "^29.4.5",
        "ts-loader": "^9.5.4",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.0"
    }
}
